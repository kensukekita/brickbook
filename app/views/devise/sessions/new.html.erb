<div class="role">
<div class="span4">

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "well"}) do |f| %>

<fieldset>
  
<legend>
Iniciar sesion
</legend>
<%= f.input :email %>
<%= f.input :password %>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "iniciar sesion" %>
  </div>
   </fieldset>
<% end %>

<%= render "devise/shared/links" %>
</div>

<div class="span8">
  <h2>Bienvenidos a BrickBook! gracias por confiar en nosotros!</h2>
</div>
</div>